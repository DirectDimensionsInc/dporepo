{
    "name": "Edan/DpoEdanBundle",
    "type": "symfony-bundle",
    "description": "Provides search capabilities against Smithsonian's EDAN API, along with UI elements.",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "Goran Halusa",
            "email": "ghalusa@quotient-inc.com",
            "homepage": "https://quotient-inc.com",
            "role": "Developer"
        }
    ],
    "autoload": {
        "psr-4": {
            "Edan\\DpoEdanBundle\\": "src/Edan/DpoEdanBundle/",
            "Smithsonian\\": "src/Smithsonian/src"
        }
    },
    "repositories": [
        {
            "type": "git",
            "url": "https://github.com/Smithsonian/php-edan-client.git"
        }
    ],
    "require": {
        "php": "^7.1",
        "smithsonian/edan-client": "dev-master",
        "symfony/asset": "^3.0|^4.0",
        "symfony/finder": "^3.0|^4.0",
        "symfony/framework-bundle": "^3.0|^4.0",
        "symfony/templating": "^3.0|^4.0",
        "symfony/yaml": "^3.0|^4.0",
    },
    "config": {
        "sort-packages": true
    }
}
